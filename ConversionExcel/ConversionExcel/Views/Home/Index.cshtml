@{
    ViewBag.Title = "Home Page";
}
@{
    Layout = null;
}

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Excelを変換するサイト</title>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    @*<script src="~/Scripts/modernizr-2.6.2.js"></script>*@
    <script type="text/javascript">
        $(function () {
            $('#add').click(function () {
                // Ajax通信を開始する
                $.ajax({
                    data: { data: "add" },
                    type: "POST",
                    url: "/Home/btnAdd_Click",
                    success: function () {
                        console.log('成功');
                    },
                    error: function () {
                        console.log('失敗');
                    }
                })
            });
        });
    </script>
</head>
<body>
    <header class="container">
        <div id="title">
            <h1>Excelを変換するサイト</h1>
        </div>
    </header>
    <form>
        <div class="container">
            <div class="form-group row">
                <label for="" class="col-sm-2 col-form-label">読込Excel：</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" id="inputPath">
                </div>
            </div>
        </div>
    </form>
    <form>
        <div id="ProcessingList" class="container">
            <div class="form-group row">
                <label for="" class="col-sm-2 col-form-label">処理内容1：</label>
                <div class="col-sm-10">
                    <select id="shori1" class="form-control">
                        <option>1月</option>
                        <option>2月</option>
                        <option>3月</option>
                        <option>4月</option>
                        <option>5月</option>
                        <option>6月</option>
                        <option>7月</option>
                        <option>8月</option>
                        <option>9月</option>
                        <option>10月</option>
                        <option>11月</option>
                        <option>12月</option>
                    </select>
                    <p class="form-inline">
                        <input type="text" class="form-control" id="argument1-1" style="width:19.6%">
                        <input type="text" class="form-control" id="argument1-2" style="width:19.6%">
                        <input type="text" class="form-control" id="argument1-3" style="width:19.6%">
                        <input type="text" class="form-control" id="argument1-4" style="width:19.6%">
                        <input type="text" class="form-control" id="argument1-5" style="width:19.6%">
                    </p>
                </div>
            </div>
        </div>
    </form>
    <footer>
        <div class="container">
            <div class="form-group row">
                <label for="" class="col-sm-2 col-form-label">出力パス：</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" id="outputPath">
                </div>
            </div>
            <div align="right">
                <button id="add" name="contents" class="btn btn-success">追加</button>
                <button id="readConfiguration" class="btn btn-success">設定読込</button>
                <button id="save" class="btn btn-success">保存</button>
                <button id="execute" class="btn btn-success">実行</button>
                <button id="exeOutPut" class="btn btn-success">exe出力</button>
            </div>
        </div>

    </footer>
</body>
</html>